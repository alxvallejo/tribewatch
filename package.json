		"autoprefixer": "^9.7.5",
		"babel-polyfill": "^6.26.0",
		"bootstrap": "^4.4.1",
		"express": "^4.16.3",
		"firebase": "^7.12.0",
		"formik": "^2.1.4",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^7.0.0",
		"postcss-loader": "^3.0.0",
		"react": "^16.5.2",
		"react-bootstrap": "^1.0.0",
		"react-dom": "^16.5.2",
		"react-firebaseui": "^4.1.0",
		"react-lines-ellipsis": "^0.14.1",
		"react-quill": "^1.3.5",
		"react-router-dom": "^5.1.2",
